                   rdfs:subClassOf prov:Agent ;
                   rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
                   rdfs:label "SoftwareAgent" ;
                   prov:category "expanded" ;
                   prov:component "agents-responsibility" ;
                   prov:definition "A software agent is running software."@en ;
                   prov:dm "http://www.w3.org/TR/2013/REC-prov-dm-20130430/#term-agent"^^xsd:anyURI ;
                   prov:n "http://www.w3.org/TR/2013/REC-prov-n-20130430/#expression-types"^^xsd:anyURI .


###  https://saref.etsi.org/core/Device
saref:Device rdf:type owl:Class ;
             rdfs:subClassOf prov:Entity ;
             rdfs:comment "A tangible object designed to accomplish a particular task. In order to accomplish this task, the device performs one or more functions. For example, a washing machine is designed to wash (task) and to accomplish this task it performs a start and stop function."@en ;
             rdfs:isDefinedBy saref: ;
             rdfs:label "Device"@en .


###  https://saref.etsi.org/saref4wear/User
s4wear:User rdf:type owl:Class ;
            owl:equivalentClass pghdprovo:Patient ;
            rdfs:subClassOf prov:Person ;
            rdfs:comment "A user is an entity using a wearable."@en ;
            rdfs:label "User"@en .


###  https://saref.etsi.org/saref4wear/Wearable
s4wear:Wearable rdf:type owl:Class ;
                rdfs:subClassOf saref:Device ;
                rdfs:comment "A wearable device is a device that is intended to be located near, on or in an organism."@en ;
                rdfs:label "Wearable device"@en .


###  https://w3id.org/pghdprovo/Application
pghdprovo:Application rdf:type owl:Class ;
                      rdfs:subClassOf prov:Entity ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty prov:wasAttributedTo ;
                                        owl:someValuesFrom prov:Person
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty pghdprovo:appName ;
                                        owl:someValuesFrom xsd:string
                                      ] ;
                      rdfs:comment "A software application is a software that can be directly executed by some processing unit." ;
                      rdfs:label "Application" .


###  https://w3id.org/pghdprovo/ContextualInfo
pghdprovo:ContextualInfo rdf:type owl:Class ;
                         rdfs:subClassOf prov:Entity ;
                         rdfs:label "contextual information" .


###  https://w3id.org/pghdprovo/PGHD
pghdprovo:PGHD rdf:type owl:Class ;
               rdfs:subClassOf prov:Entity ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty prov:wasDerivedFrom ;
                                 owl:someValuesFrom s4wear:Wearable
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty pghdprovo:hasContextualInfo ;
                                 owl:someValuesFrom pghdprovo:ContextualInfo
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty pghdprovo:wasCollectedBy ;
                                 owl:someValuesFrom prov:Person
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty prov:wasAttributedTo ;
                                 owl:allValuesFrom pghdprovo:Patient
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty prov:wasGeneratedBy ;
                                 owl:allValuesFrom pghdprovo:PGHDRequest
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty pghdprovo:dataSource ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:string
                               ] ;
               rdfs:comment "Patient Generated Health Data (PGHD) a term referring to data that patients (or their caregivers) collect from home or any location outside the clinical environment." ;
               rdfs:label "PGHD" .


###  https://w3id.org/pghdprovo/PGHDRequest
pghdprovo:PGHDRequest rdf:type owl:Class ;
                      rdfs:subClassOf prov:Activity ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty prov:used ;
                                        owl:someValuesFrom pghdprovo:Application
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty pghdprovo:PGHDRequestedBy ;
